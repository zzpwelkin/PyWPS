<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0"
xmlns:ows="http://www.opengis.net/ows/1.1"
xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd"
 service="WPS" version="1.0.0" xml:lang="en-US"> 
	<ProcessDescription wps:processVersion="1" storeSupported="true" statusSupported="true">
		<ows:Identifier>v.distance</ows:Identifier>
		<ows:Title>Finds the nearest element in vector map 'to' for elements in vector map 'from'.</ows:Title>
		<ows:Abstract>http://grass.osgeo.org/grass70/manuals/html70_user/v.distance.html</ows:Abstract>
		<ows:Metadata xlink:title="vector" />
		<ows:Metadata xlink:title="database" />
		<ows:Metadata xlink:title="attribute table" />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>from</ows:Identifier>
				<ows:Title>Name of existing vector map (from)</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>from_layer</ows:Identifier>
				<ows:Title>A single vector map can be connected to multiple database tables. This number determines which table to use. When used with direct OGR access this is the layer name.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1024">
				<ows:Identifier>from_type</ows:Identifier>
				<ows:Title>Feature type</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>point</ows:Value>
						<ows:Value>centroid</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>point</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>to</ows:Identifier>
				<ows:Title>Name of existing vector map (to)</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>to_layer</ows:Identifier>
				<ows:Title>A single vector map can be connected to multiple database tables. This number determines which table to use. When used with direct OGR access this is the layer name.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1024">
				<ows:Identifier>to_type</ows:Identifier>
				<ows:Title>Feature type</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>point</ows:Value>
						<ows:Value>line</ows:Value>
						<ows:Value>boundary</ows:Value>
						<ows:Value>centroid</ows:Value>
						<ows:Value>area</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>point,line,area</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>dmax</ows:Identifier>
				<ows:Title>Maximum distance or -1 for no limit</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>-1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>dmin</ows:Identifier>
				<ows:Title>Minimum distance or -1 for no limit</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>-1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1024">
				<ows:Identifier>upload</ows:Identifier>
				<ows:Title>Values describing the relation between two nearest features</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>cat</ows:Value>
						<ows:Value>dist</ows:Value>
						<ows:Value>to_x</ows:Value>
						<ows:Value>to_y</ows:Value>
						<ows:Value>to_along</ows:Value>
						<ows:Value>to_angle</ows:Value>
						<ows:Value>to_attr</ows:Value>
					</ows:AllowedValues>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1024">
				<ows:Identifier>column</ows:Identifier>
				<ows:Title>Column name(s) where values specified by 'upload' option will be uploaded</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>to_column</ows:Identifier>
				<ows:Title>Column name of nearest feature (used with upload=to_attr)</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>table</ows:Identifier>
				<ows:Title>Name of table created when the 'distance to all' flag is used</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-p</ows:Identifier>
				<ows:Title>First column is always category of 'from' feature called from_cat</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-a</ows:Identifier>
				<ows:Title>Output is written to stdout but may be uploaded to a new table created by this module; multiple 'upload' options may be used.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>output</ows:Identifier>
				<ows:Title>Name for output vector map containing lines connecting nearest elements</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
