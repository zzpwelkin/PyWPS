					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>gauss</ows:Value>
						<ows:Value>lu</ows:Value>
						<ows:Value>jacobi</ows:Value>
						<ows:Value>sor</ows:Value>
						<ows:Value>bicgstab</ows:Value>
					</ows:AllowedValues>
