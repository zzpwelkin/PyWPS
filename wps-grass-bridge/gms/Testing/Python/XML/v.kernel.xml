<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0"
xmlns:ows="http://www.opengis.net/ows/1.1"
xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd"
 service="WPS" version="1.0.0" xml:lang="en-US"> 
	<ProcessDescription wps:processVersion="1" storeSupported="true" statusSupported="true">
		<ows:Identifier>v.kernel</ows:Identifier>
		<ows:Title>Generates a raster density map from vector point data using a moving kernel or optionally generates a vector density map on a vector network.</ows:Title>
		<ows:Abstract>http://grass.osgeo.org/grass70/manuals/html70_user/v.kernel.html</ows:Abstract>
		<ows:Metadata xlink:title="vector" />
		<ows:Metadata xlink:title="kernel density" />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>input</ows:Identifier>
				<ows:Title>Input vector with training points</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>net</ows:Identifier>
				<ows:Title>Input network vector map</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>output</ows:Identifier>
				<ows:Title>Output raster/vector map</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>stddeviation</ows:Identifier>
				<ows:Title>Standard deviation in map units</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>dsize</ows:Identifier>
				<ows:Title>Discretization error in map units</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>0.</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>segmax</ows:Identifier>
				<ows:Title>Maximum length of segment on network</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>100.</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>distmax</ows:Identifier>
				<ows:Title>Maximum distance from point to network</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>100.</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>mult</ows:Identifier>
				<ows:Title>Multiply the density result by this number</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:float">float</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1.</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>node</ows:Identifier>
				<ows:Title>Node method</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>none</ows:Value>
						<ows:Value>split</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>none</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>kernel</ows:Identifier>
				<ows:Title>Kernel function</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>uniform</ows:Value>
						<ows:Value>triangular</ows:Value>
						<ows:Value>epanechnikov</ows:Value>
						<ows:Value>quartic</ows:Value>
						<ows:Value>triweight</ows:Value>
						<ows:Value>gaussian</ows:Value>
						<ows:Value>cosine</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>gaussian</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-o</ows:Identifier>
				<ows:Title>Try to calculate an optimal standard deviation with 'stddeviation' taken as maximum (experimental)</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-q</ows:Identifier>
				<ows:Title>Only calculate optimal standard deviation and exit (no map is written)</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-n</ows:Identifier>
				<ows:Title>In network mode, normalize values by sum of density multiplied by length of each segment. Integral over the output map then gives 1.0 * mult</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-m</ows:Identifier>
				<ows:Title>In network mode, multiply the result by number of input points.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>stdout</ows:Identifier>
				<ows:Title>Module output on stdout</ows:Title>
				<ows:Abstract>The output of the module written to stdout</ows:Abstract>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
