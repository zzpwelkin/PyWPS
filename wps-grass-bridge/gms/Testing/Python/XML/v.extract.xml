<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0"
xmlns:ows="http://www.opengis.net/ows/1.1"
xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.opengis.net/wps/1.0.0
 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd"
 service="WPS" version="1.0.0" xml:lang="en-US"> 
	<ProcessDescription wps:processVersion="1" storeSupported="true" statusSupported="true">
		<ows:Identifier>v.extract</ows:Identifier>
		<ows:Title>Selects vector features from an existing vector map and creates a new vector map containing only the selected features.</ows:Title>
		<ows:Abstract>http://grass.osgeo.org/grass70/manuals/html70_user/v.extract.html</ows:Abstract>
		<ows:Metadata xlink:title="vector" />
		<ows:Metadata xlink:title="extract" />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>input</ows:Identifier>
				<ows:Title>Or data source for direct OGR access</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/dgn</MimeType>
						</Format>
						<Format>
							<MimeType>application/shp</MimeType>
						</Format>
						<Format>
							<MimeType>application/x-zipped-shp</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>layer</ows:Identifier>
				<ows:Title>A single vector map can be connected to multiple database tables. This number determines which table to use. When used with direct OGR access this is the layer name.</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1024">
				<ows:Identifier>type</ows:Identifier>
				<ows:Title>Feature type</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>point</ows:Value>
						<ows:Value>line</ows:Value>
						<ows:Value>boundary</ows:Value>
						<ows:Value>centroid</ows:Value>
						<ows:Value>area</ows:Value>
						<ows:Value>face</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>point,line,boundary,centroid,area,face</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>cats</ows:Identifier>
				<ows:Title>Example: 1,3,7-9,13</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>where</ows:Identifier>
				<ows:Title>Example: income &lt; 1000 and inhab &gt;= 10000</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:string">string</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>file</ows:Identifier>
				<ows:Title>If '-' given reads from standard input</ows:Title>
				<ComplexData maximumMegabytes="2048">
					<Default>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/plain</MimeType>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>random</ows:Identifier>
				<ows:Title>Number must be smaller than unique cat count in layer</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer">integer</ows:DataType>
					<ows:AnyValue/>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>new</ows:Identifier>
				<ows:Title>If new &gt;= 0, attributes is not copied</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:integer">integer</ows:DataType>
					<ows:AnyValue/>
					<DefaultValue>-1</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-d</ows:Identifier>
				<ows:Title>Dissolve common boundaries (default is no)</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-t</ows:Identifier>
				<ows:Title>Do not copy attributes (see also 'new' parameter)</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
			<Input minOccurs="0" maxOccurs="1">
				<ows:Identifier>-r</ows:Identifier>
				<ows:Title>Reverse selection</ows:Title>
				<LiteralData>
					<ows:DataType ows:reference="xs:boolean">boolean</ows:DataType>
					<ows:AllowedValues>
						<ows:Value>true</ows:Value>
						<ows:Value>false</ows:Value>
					</ows:AllowedValues>
					<DefaultValue>false</DefaultValue>
				</LiteralData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>output</ows:Identifier>
				<ows:Title>Name for output vector map</ows:Title>
				<ComplexOutput>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/3.1.1/base/gml.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>application/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/gml/2.1.2/feature.xsd</Schema>
						</Format>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/kml/2.2.0/ogckml22.xsd</Schema>
						</Format>
					</Supported>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
</wps:ProcessDescriptions>
